items-per-page="::receiversCtrl.itemsPerPage"
data-ng-model="receiversCtrl.page"
max-size="::receiversCtrl.maxSize"
class="pagination-sm"
boundary-links="true"
force-ellipses="true"
data-ng-show="receiversCtrl.numItems > receiversCtrl.itemsPerPage">
</ul>